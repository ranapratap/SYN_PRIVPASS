<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/app_bg_color"
   >

    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_margin="20dp"
        android:background="#ADADC2"
        android:orientation="vertical" >

        <TextView
            android:layout_width="fill_parent"
            android:layout_height="2dp"
            android:background="@color/edit_bg" />

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="2dp"
                android:layout_height="fill_parent"
                android:background="@color/edit_bg" />

            <com.spinlogics.icrisat.Pothana
                android:id="@+id/animal_type"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1.5"
                android:gravity="center"
                android:text="TYPE"
                android:textColor="@android:color/white"
                android:textSize="28dp"
                android:textStyle="bold" />

            <TextView
                android:layout_width="2dp"
                android:layout_height="fill_parent"
                android:background="@color/edit_bg" />

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="0.5"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="DM"
                    android:textColor="@android:color/black"
                    android:textSize="28dp" />

                <TextView
                    android:layout_width="2dp"
                    android:layout_height="fill_parent"
                    android:background="@color/edit_bg" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="CP"
                    android:textColor="@android:color/black"
                    android:textSize="28dp" />

                <TextView
                    android:layout_width="2dp"
                    android:layout_height="fill_parent"
                    android:background="@color/edit_bg" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="ME"
                    android:textColor="@android:color/black"
                    android:textSize="28dp" />

                <TextView
                    android:layout_width="2dp"
                    android:layout_height="fill_parent"
                    android:background="@color/edit_bg" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="CA"
                    android:textColor="@android:color/black"
                    android:textSize="28dp" />

                <TextView
                    android:layout_width="2dp"
                    android:layout_height="fill_parent"
                    android:background="@color/edit_bg" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="P"
                    android:textColor="@android:color/black"
                    android:textSize="28dp" />

                <TextView
                    android:layout_width="2dp"
                    android:layout_height="fill_parent"
                    android:background="@color/edit_bg" />
            </LinearLayout>
        </LinearLayout>

        <TextView
            android:layout_width="fill_parent"
            android:layout_height="2dp"
            android:background="@color/edit_bg" />

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="2dp"
                android:layout_height="fill_parent"
                android:background="@color/edit_bg" />

            <com.spinlogics.icrisat.Pothana
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1.5"
                android:gravity="center"
                android:text="మొత్తం"
                android:textColor="@android:color/black"
                android:textSize="28dp" />

            <TextView
                android:layout_width="2dp"
                android:layout_height="fill_parent"
                android:background="@color/edit_bg" />

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="0.5"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/total_dm"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="DM"
                    android:textColor="@color/edit_bg"
                    android:textSize="28dp" />

                <TextView
                    android:layout_width="2dp"
                    android:layout_height="fill_parent"
                    android:background="@color/edit_bg" />

                <TextView
                    android:id="@+id/total_cp"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="DM"
                    android:textColor="@color/edit_bg"
                    android:textSize="28dp" />

                <TextView
                    android:layout_width="2dp"
                    android:layout_height="fill_parent"
                    android:background="@color/edit_bg" />

                <TextView
                    android:id="@+id/total_me"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="DM"
                    android:textColor="@color/edit_bg"
                    android:textSize="28dp" />

                <TextView
                    android:layout_width="2dp"
                    android:layout_height="fill_parent"
                    android:background="@color/edit_bg" />

                <TextView
                    android:id="@+id/total_ca"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="DM"
                    android:textColor="@color/edit_bg"
                    android:textSize="28dp" />

                <TextView
                    android:layout_width="2dp"
                    android:layout_height="fill_parent"
                    android:background="@color/edit_bg" />

                <TextView
                    android:id="@+id/total_p"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="DM"
                    android:textColor="@color/edit_bg"
                    android:textSize="28dp" />

                <TextView
                    android:layout_width="2dp"
                    android:layout_height="fill_parent"
                    android:background="@color/edit_bg" />
            </LinearLayout>
        </LinearLayout>

        <TextView
            android:layout_width="fill_parent"
            android:layout_height="2dp"
            android:background="@color/edit_bg" />

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="2dp"
                android:layout_height="fill_parent"
                android:background="@color/edit_bg" />

            <com.spinlogics.icrisat.Pothana
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1.5"
                android:gravity="center"
                android:text="Given"
                android:textColor="@android:color/black"
                android:textSize="28dp" />

            <TextView
                android:layout_width="2dp"
                android:layout_height="fill_parent"
                android:background="@color/edit_bg" />

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="0.5"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/given_dm"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="DM"
                    android:textColor="@color/edit_bg"
                    android:textSize="28dp" />

                <TextView
                    android:layout_width="2dp"
                    android:layout_height="fill_parent"
                    android:background="@color/edit_bg" />

                <TextView
                    android:id="@+id/given_cp"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="DM"
                    android:textColor="@color/edit_bg"
                    android:textSize="28dp" />

                <TextView
                    android:layout_width="2dp"
                    android:layout_height="fill_parent"
                    android:background="@color/edit_bg" />

                <TextView
                    android:id="@+id/given_me"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="DM"
                    android:textColor="@color/edit_bg"
                    android:textSize="28dp" />

                <TextView
                    android:layout_width="2dp"
                    android:layout_height="fill_parent"
                    android:background="@color/edit_bg" />

                <TextView
                    android:id="@+id/given_ca"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="DM"
                    android:textColor="@color/edit_bg"
                    android:textSize="28dp" />

                <TextView
                    android:layout_width="2dp"
                    android:layout_height="fill_parent"
                    android:background="@color/edit_bg" />

                <TextView
                    android:id="@+id/given_p"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="DM"
                    android:textColor="@color/edit_bg"
                    android:textSize="28dp" />

                <TextView
                    android:layout_width="2dp"
                    android:layout_height="fill_parent"
                    android:background="@color/edit_bg" />
            </LinearLayout>
        </LinearLayout>

        <TextView
            android:layout_width="fill_parent"
            android:layout_height="2dp"
            android:background="@color/edit_bg" />

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="2dp"
                android:layout_height="fill_parent"
                android:background="@color/edit_bg" />

            <com.spinlogics.icrisat.Pothana
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1.5"
                android:gravity="center"
                android:text="Balance"
                android:textColor="@android:color/black"
                android:textSize="28dp" />

            <TextView
                android:layout_width="2dp"
                android:layout_height="fill_parent"
                android:background="@color/edit_bg" />

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="0.5"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/balance_dm"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="DM"
                    android:textColor="@color/edit_bg"
                    android:textSize="28dp" />

                <TextView
                    android:layout_width="2dp"
                    android:layout_height="fill_parent"
                    android:background="@color/edit_bg" />

                <TextView
                    android:id="@+id/balance_cp"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="DM"
                    android:textColor="@color/edit_bg"
                    android:textSize="28dp" />

                <TextView
                    android:layout_width="2dp"
                    android:layout_height="fill_parent"
                    android:background="@color/edit_bg" />

                <TextView
                    android:id="@+id/balance_me"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="DM"
                    android:textColor="@color/edit_bg"
                    android:textSize="28dp" />

                <TextView
                    android:layout_width="2dp"
                    android:layout_height="fill_parent"
                    android:background="@color/edit_bg" />

                <TextView
                    android:id="@+id/balance_ca"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="DM"
                    android:textColor="@color/edit_bg"
                    android:textSize="28dp" />

                <TextView
                    android:layout_width="2dp"
                    android:layout_height="fill_parent"
                    android:background="@color/edit_bg" />

                <TextView
                    android:id="@+id/balance_p"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="DM"
                    android:textColor="@color/edit_bg"
                    android:textSize="28dp" />

                <TextView
                    android:layout_width="2dp"
                    android:layout_height="fill_parent"
                    android:background="@color/edit_bg" />
            </LinearLayout>
        </LinearLayout>

        <TextView
            android:layout_width="fill_parent"
            android:layout_height="2dp"
            android:background="@color/edit_bg" />
    </LinearLayout>

    <Button
        android:id="@+id/next_goo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/linearLayout1"
        android:layout_below="@+id/linearLayout1"
        android:layout_margin="20dp"
        android:layout_marginRight="14dp"
        android:background="@android:color/darker_gray"
        android:gravity="center"
        android:padding="10dp"
        android:text="Next"
        android:textSize="28dp" />

</RelativeLayout>